            ]
        },
        "feels_like_value": {
            "selectors": [
                {
                    "type": "class",
                    "value": "feels-like-text"
                }
            ],
            "transform": "return input.replace('&nbsp;', '').replace(/[a-zA-Z\\.\\:\\s+\\˚]/g, '')"
        },
        "low_value": {
            "selectors": [
                {
                    "type": "class",
                    "value": "low-temp-text"
                }
            ],
            "transform": "return input.replace('&nbsp;', '').replace(/[a-zA-Z\\.\\:\\s+\\˚]/g, '')"
        },
        "high_value": {
            "selectors": [
                {
                    "type": "class",
                    "value": "high-temp-text"
                }
            ],
            "transform": "return input.replace('&nbsp;', '').replace(/[a-zA-Z\\.\\:\\s+\\˚]/g, '')"
        },
        "temperature_value": {
            "selectors": [
                {
                    "type": "class",
                    "value": "currently"
                },
                {
                    "type": "class",
                    "value": "desc"
                },
                {
                    "type": "class",
                    "value": "summary"
                }
            ],
            "transform": "return input.replace('&nbsp;', '').replace(/[a-zA-Z\\.\\:\\s+\\˚]/g, '')"
        },
        "temperature_description": {
            "selectors": [
                {
                    "type": "class",
                    "value": "currently"
                },
                {
                    "type": "class",
                    "value": "desc"
                },
                {
                    "type": "class",
                    "value": "summary"
                }
            ],
            "transform": "return input.replace('&nbsp;', '').replace(/[^a-zA-Z ]/g, '')"
        },
        "temperature_condition": {
            "selectors": [
                {
                    "type": "class",
                    "value": "currently"
                },
                {
                    "type": "class",
                    "value": "currently-icon"
                },
                {
                    "type": "tag",
                    "value": "img"
                }
            ],
            "attribute": "src",
            "transform": "return input.substring(input.lastIndexOf('/') + 1, input.lastIndexOf('.'))"
        }
    }
